{
  "name": "brokle-anthropic",
  "version": "0.1.2",
  "description": "Anthropic SDK wrapper with automatic Brokle tracing",
  "author": "Brokle AI",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "brokle": "workspace:*"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "@types/node": "^20.10.0",
    "brokle": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brokle-ai/brokle-js.git",
    "directory": "packages/brokle-anthropic"
  },
  "homepage": "https://github.com/brokle-ai/brokle-js/tree/main/packages/brokle-anthropic#readme",
  "bugs": {
    "url": "https://github.com/brokle-ai/brokle-js/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "brokle",
    "anthropic",
    "claude",
    "observability",
    "tracing",
    "opentelemetry",
    "llm",
    "ai"
  ]
}
