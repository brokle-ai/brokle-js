{
  "name": "brokle",
  "version": "0.3.0",
  "description": "OpenTelemetry-native observability SDK for AI applications",
  "author": "Brokle AI",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./openai": {
      "types": "./dist/integrations/openai/index.d.ts",
      "import": "./dist/integrations/openai/index.js",
      "require": "./dist/integrations/openai/index.cjs"
    },
    "./anthropic": {
      "types": "./dist/integrations/anthropic/index.d.ts",
      "import": "./dist/integrations/anthropic/index.js",
      "require": "./dist/integrations/anthropic/index.cjs"
    },
    "./langchain": {
      "types": "./dist/integrations/langchain/index.d.ts",
      "import": "./dist/integrations/langchain/index.js",
      "require": "./dist/integrations/langchain/index.cjs"
    },
    "./scorers": {
      "types": "./dist/scorers/index.d.ts",
      "import": "./dist/scorers/index.js",
      "require": "./dist/scorers/index.cjs"
    },
    "./vercel-ai": {
      "types": "./dist/integrations/vercel-ai/index.d.ts",
      "import": "./dist/integrations/vercel-ai/index.js",
      "require": "./dist/integrations/vercel-ai/index.cjs"
    },
    "./google": {
      "types": "./dist/integrations/google/index.d.ts",
      "import": "./dist/integrations/google/index.js",
      "require": "./dist/integrations/google/index.cjs"
    },
    "./mistral": {
      "types": "./dist/integrations/mistral/index.d.ts",
      "import": "./dist/integrations/mistral/index.js",
      "require": "./dist/integrations/mistral/index.cjs"
    },
    "./azure": {
      "types": "./dist/integrations/azure/index.d.ts",
      "import": "./dist/integrations/azure/index.js",
      "require": "./dist/integrations/azure/index.cjs"
    },
    "./bedrock": {
      "types": "./dist/integrations/bedrock/index.d.ts",
      "import": "./dist/integrations/bedrock/index.js",
      "require": "./dist/integrations/bedrock/index.cjs"
    },
    "./cohere": {
      "types": "./dist/integrations/cohere/index.d.ts",
      "import": "./dist/integrations/cohere/index.js",
      "require": "./dist/integrations/cohere/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "openai": [
        "./dist/integrations/openai/index.d.ts"
      ],
      "anthropic": [
        "./dist/integrations/anthropic/index.d.ts"
      ],
      "langchain": [
        "./dist/integrations/langchain/index.d.ts"
      ],
      "scorers": [
        "./dist/scorers/index.d.ts"
      ],
      "vercel-ai": [
        "./dist/integrations/vercel-ai/index.d.ts"
      ],
      "google": [
        "./dist/integrations/google/index.d.ts"
      ],
      "mistral": [
        "./dist/integrations/mistral/index.d.ts"
      ],
      "azure": [
        "./dist/integrations/azure/index.d.ts"
      ],
      "bedrock": [
        "./dist/integrations/bedrock/index.d.ts"
      ],
      "cohere": [
        "./dist/integrations/cohere/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "clean": "rm -rf dist node_modules",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "openai": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "@anthropic-ai/sdk": ">=0.30.0",
    "@langchain/core": ">=0.3.0",
    "ai": ">=3.0.0 || >=4.0.0",
    "@google/genai": ">=1.0.0",
    "@mistralai/mistralai": ">=1.0.0",
    "@aws-sdk/client-bedrock-runtime": ">=3.0.0",
    "cohere-ai": ">=7.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@langchain/core": {
      "optional": true
    },
    "ai": {
      "optional": true
    },
    "@google/genai": {
      "optional": true
    },
    "@mistralai/mistralai": {
      "optional": true
    },
    "@aws-sdk/client-bedrock-runtime": {
      "optional": true
    },
    "cohere-ai": {
      "optional": true
    }
  },
  "dependencies": {
    "@opentelemetry/exporter-logs-otlp-proto": "^0.208.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "mustache": "^4.2.0",
    "nunjucks": "^3.2.4"
  },
  "optionalDependencies": {
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.208.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.208.0"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.6",
    "@types/node": "^20.17.0",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.0.0",
    "openai": "^4.0.0",
    "@anthropic-ai/sdk": "^0.30.0",
    "@langchain/core": "^0.3.80",
    "prettier": "^3.7.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.0",
    "vitest": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brokle-ai/brokle-js.git"
  },
  "homepage": "https://github.com/brokle-ai/brokle-js#readme",
  "bugs": {
    "url": "https://github.com/brokle-ai/brokle-js/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "opentelemetry",
    "otel",
    "observability",
    "tracing",
    "ai",
    "llm",
    "genai",
    "monitoring",
    "apm",
    "openai",
    "anthropic",
    "langchain",
    "vercel-ai",
    "ai-sdk",
    "google-genai",
    "gemini",
    "mistral",
    "azure-openai",
    "bedrock",
    "cohere",
    "rag"
  ]
}
